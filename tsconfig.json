{
  "compilerOptions": {
    "jsx": "react",
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src", "api"]
}
