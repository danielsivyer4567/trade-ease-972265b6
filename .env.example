# Supabase Configuration
# Get these from your Supabase project dashboard -> Project Settings -> API
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Vercel Configuration
# Get these from your Vercel dashboard
VERCEL_TOKEN=your_vercel_deployment_token
VERCEL_ORG_ID=your_vercel_org_id
VERCEL_PROJECT_ID=your_vercel_project_id

# Database (for migrations)
DATABASE_URL=your_database_url

# Environment
VITE_APP_ENV=development

# Google Maps API (REQUIRED)
# Get from: https://console.cloud.google.com/apis/credentials
# Must enable: Maps JavaScript API, Geocoding API, Places API, Directions API, Distance Matrix API
# Required setup:
# 1. Create a project in Google Cloud Console
# 2. Enable billing (required even for free tier)
# 3. Enable the required APIs
# 4. Create an API key with appropriate restrictions
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Mapbox APIs (Alternative/Fallback)
# Get token at: https://account.mapbox.com/access-tokens/
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here

# ArcGIS (Geographic services - alternative to Google Maps)
# Get from: https://developers.arcgis.com/
ARCGIS_API_KEY=your_arcgis_api_key
VITE_ARCGIS_API_KEY=your_arcgis_api_key_here

# Real Estate APIs (Optional - for property value/details)
VITE_DOMAIN_API_KEY=your_domain_api_key_here
VITE_REALESTATE_API_KEY=your_realestate_api_key_here

# ABN Lookup (Australian Business Number)
ABN_GUID=your_abn_guid
ABN_PROXY_URL=your_proxy_url

# Twilio Configuration (PREMIUM FEATURE)
# Note: Twilio integration is now a premium feature managed at platform level
# Users don't need to configure these - they're auto-enabled for paid plans
VITE_TWILIO_SID=your-twilio-sid
VITE_TWILIO_AUTH_TOKEN=your-twilio-auth-token
VITE_TWILIO_PHONE_NUMBER=your-twilio-phone-number
# TWILIO_SID=your_twilio_sid
# TWILIO_AUTH_TOKEN=your_twilio_auth_token
# TWILIO_PHONE_NUMBER=your_twilio_phone_number
# TWILIO_ACCOUNT_SID=your_twilio_sid

# OpenAI API (PREMIUM FEATURE)
# Note: OpenAI integration is now a premium feature managed at platform level
# Users don't need to configure these - they're auto-enabled for paid plans
# ⚠️ SECURITY WARNING: Never share or commit your actual API key!
# 1. Get a new API key from: https://platform.openai.com/api-keys  
# 2. Add it to your .env.local file (this file is ignored by git)
# 3. Keep your key secret - treat it like a password
OPENAI_API_KEY=your-openai-api-key-here
REACT_APP_OPENAI_API_KEY=your-openai-api-key-here

# Google Cloud Speech-to-Text API Key (Alternative to OpenAI)
REACT_APP_GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key_here

# Mailgun (Email service)
# Get these from your Mailgun dashboard
# 1. API Key: From Settings -> API Keys
# 2. Domain: Your verified sending domain
MAILGUN_API_KEY=your_mailgun_api_key
MAILGUN_DOMAIN=your_mailgun_domain

# Brevo (Alternative Email service)
# Get this from your Brevo dashboard -> SMTP & API -> API Keys
BREVO_API_KEY=your_brevo_api_key

# DocuSeal (Document signing)
# Get from: https://www.docuseal.co/
VITE_DOCUSEAL_API_KEY=your_docuseal_api_key
VITE_DOCUSEAL_API_URL=https://api.docuseal.co
VITE_DOCUSEAL_WEBHOOK_SECRET=your_webhook_secret

# N8N (Workflow automation)
# Set this to your n8n instance URL
VITE_N8N_URL=http://localhost:5678
N8N_API_KEY=n8n_api_6209b969611232dbcf1350a6cf8b4258a82a2c43903cbe44c694c06a35032ecb16603d8c6e75f1e1

# Admin functionality
ADMIN_SECRET_KEY=your_admin_secret_key

# ElevenLabs API Key for realistic voice synthesis
# Get your API key from https://elevenlabs.io/
# Free tier includes 10,000 characters per month
VITE_ELEVEN_LABS_API_KEY=your_elevenlabs_api_key_here

# Use Free TTS Services 
# Set to 'true' to use free Australian TTS voices instead of ElevenLabs
VITE_USE_FREE_TTS=true

# Azure Cognitive Services TTS (FREE TIER: 5 hours/month)
# Best quality Australian voices: en-AU-NatashaNeural (female), en-AU-WilliamNeural (male)
# Get free key at: https://azure.microsoft.com/free/cognitive-services/
VITE_AZURE_TTS_KEY=your_azure_tts_key_here
VITE_AZURE_TTS_REGION=australiaeast

# OpenAI TTS (PAID: $15/1M characters, very natural voices)
# Alternative option for high-quality voices
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Key (REQUIRED for AI Live Assistant)
# Get from: https://aistudio.google.com/app/apikey
# Free tier includes generous limits for testing
# ⚠️ SECURITY WARNING: Never share or commit your actual API key!
VITE_GEMINI_API_KEY=your_gemini_api_key_here

